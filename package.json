{
  "name": "noroff-game",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com/NancyBolstad/game.git",
  "author": "Nancy Bolstad <litchacha@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "browserify -p tsify src/index.ts > docs/bundle.js && node-sass src/styles/index.scss -o docs/css/",
    "build:scripts": "browserify -p tsify src/index.ts > docs/bundle.js",
    "build:styles": "node-sass src/styles/index.scss -o docs/css/",
    "watch": "watchify -p tsify src/index.ts -o docs/bundle.js -d && node-sass --watch src/styles/index.scss -o docs/css/",
    "watch:ts": "watchify -p tsify src/index.ts -o docs/bundle.js -d",
    "watch:scss": "node-sass --watch src/styles/index.scss -o docs/css/"
  },
  "dependencies": {
    "browserify": "^12.0.1",
    "tsify": "*",
    "typescript": "~1.8.7",
    "watchify": "^3.11.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "bootstrap": "^4.3.1",
    "husky": "^3.0.8",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass-loader": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bugs": {
    "url": "https://github.com/NancyBolstad/game/issues"
  },
  "homepage": "https://github.com/NancyBolstad/game#readme"
}
