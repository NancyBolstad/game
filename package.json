{
  "name": "noroff-game",
  "version": "0.0.1",
  "private": true,
  "repository": "git@github.com/NancyBolstad/game.git",
  "author": "Nancy Bolstad <litchacha@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve src/index.html --open",
    "watch": "parcel watch src/*.html",
    "clean": "rm -rf dist/",
    "build": "rm -rf dist/ && parcel build src/*.html",
    "build-github-page": "rm -rf doc/ && parcel build src/*.html --out-dir doc/ --no-minify"
  },
  "dependencies": {
    "autoprefixer": "^9.7.2",
    "browserify": "^12.0.1",
    "clean-css-cli": "^4.3.0",
    "husky": "^3.0.8",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsify": "*",
    "typescript": "^3.7.2",
    "watchify": "^3.11.1"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bugs": {
    "url": "https://github.com/NancyBolstad/game/issues"
  },
  "homepage": "https://github.com/NancyBolstad/game#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  }
}
